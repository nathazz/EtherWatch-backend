{
  "extends": "@tsconfig/node23/tsconfig.json",
  "compilerOptions": {
    "lib": [
      "es2024",
      "ESNext.Array",
      "ESNext.Collection",
      "ESNext.Iterator",
      "ESNext.Promise"
    ],
    "module": "nodenext",
    "target": "es2024",
    "resolveJsonModule": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node16"
  },
  "include": ["src/**/*", "src/http/coinGecko/ethInfos.ts"],
  "exclude": ["node_modules"]
}
